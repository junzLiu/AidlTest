[32m+[m[32mpackage com.mark.aidl.service;[m
[32m+[m
[32m+[m[32mimport android.app.Service;[m
[32m+[m[32mimport android.content.Intent;[m
[32m+[m[32mimport android.os.IBinder;[m
[32m+[m[32mimport android.os.Process;[m
[32m+[m[32mimport android.os.RemoteException;[m
[32m+[m[32mimport android.support.annotation.Nullable;[m
[32m+[m
[32m+[m[32mimport com.mark.aidl.IMyAidlInterface;[m
[32m+[m
[32m+[m[32mimport java.util.Calendar;[m
[32m+[m[32mimport java.util.Random;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by Mark on 2017/12/22.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class RemoteService extends Service {[m
[32m+[m
[32m+[m[32m    @Nullable[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public IBinder onBind(Intent intent) {[m
[32m+[m[32m        return mBinder;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    IMyAidlInterface.Stub mBinder = new IMyAidlInterface.Stub() {[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public void basicTypes(int anInt, long aLong, boolean aBoolean, float aFloat, double aDouble, String aString) throws RemoteException {[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public int getPid() throws RemoteException {[m
[32m+[m[32m            return Process.myPid();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public int getRandomNum() throws RemoteException {[m
[32m+[m[32m            return new Random(Calendar.getInstance().getTimeInMillis()).nextInt();[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
